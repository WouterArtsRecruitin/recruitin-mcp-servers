# Environment Variables Template for Render Deployment
# Copy these to Render Environment Variables

# REQUIRED: Anthropic Claude API Key
CLAUDE_API_KEY=your_claude_api_key_here

# OPTIONAL: Notion Integration (voor rapport opslag)
NOTION_TOKEN=your_notion_token_here
NOTION_DATABASE_ID=your_database_id_here

# PRODUCTION SETTINGS
NODE_ENV=production
PORT=3000

# JOTFORM WEBHOOK SETTINGS (configured in Render)
JOTFORM_WEBHOOK_SECRET=your_webhook_secret_here

# LOG LEVEL
LOG_LEVEL=info